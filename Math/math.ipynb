   "source": [
    "Given an integer x, return true if x is a palindrome, and false otherwise."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "class Solution:\n",
    "    def isPalindrome(self, x: int) -> bool:\n",
    "        y = str(x)\n",
    "        if y == y[ : :-1]:\n",
    "            return True\n",
    "\n",
    "        else:\n",
    "            return False\n",
    "        "
   ]
  }
